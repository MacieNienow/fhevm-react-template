{
  "name": "fhevm-react-template",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK - Framework-agnostic encryption utilities for confidential frontends",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspace=examples/nextjs-example",
    "test": "npm run test --workspaces",
    "type-check": "npm run type-check --workspaces",
    "clean": "rm -rf node_modules packages/*/node_modules examples/*/node_modules"
  },
  "keywords": [
    "fhevm",
    "zama",
    "fhe",
    "encryption",
    "confidential",
    "privacy",
    "blockchain",
    "ethereum",
    "sdk",
    "react",
    "nextjs",
    "vue"
  ],
  "author": "FHEVM Community",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
