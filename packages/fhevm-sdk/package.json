{
  "name": "@fhevm/sdk",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK for building confidential frontends - framework-agnostic encryption and decryption utilities",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/react.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/react.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "fhevm",
    "zama",
    "fhe",
    "encryption",
    "confidential",
    "privacy",
    "blockchain",
    "ethereum",
    "sdk"
  ],
  "author": "FHEVM Community",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "fhevmjs": "^0.5.0",
    "viem": "^2.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "react": "^18.3.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0"
  }
}
